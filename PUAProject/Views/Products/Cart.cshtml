@model List<PUAProject.ViewModels.CartContentViewModel>

<html lang="en">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Cart Items</title>
</head>
<body>
    <table cellpadding="4" cellspacing="5" border="1">
        <tr>
            <th>Name</th>
            <th>Photo</th>
            <th>Price</th>
            <th>Quantity</th>
            <th>Sub Total</th>
        </tr>
@foreach (var item in Model){
 <tr>

                <td>@item.ProductTitle</td>
                <td><img src="@item.ImageUrl" width="60" /> </td>
                <td>@item.Price</td>
                <td>@item.Quantity</td>
                <td>@(item.Price * item.Quantity)</td>
            </tr>
        }

    </table>
    <br>
    <a asp-controller="Home" asp-action="Index">Continue Shopping</a>

</body>
</html>